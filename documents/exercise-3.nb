Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"ua", "[", 
    RowBox[{"q_", ",", "p_", ",", "h_"}], "]"}], ":=", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"q", "+", 
      RowBox[{"h", " ", 
       RowBox[{"p", "/", "m"}]}]}], ",", "p"}], "}"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"ub", "[", 
    RowBox[{"q_", ",", "p_", ",", "h_"}], "]"}], ":=", 
   RowBox[{"{", 
    RowBox[{"q", ",", 
     RowBox[{"p", "-", 
      RowBox[{"h", " ", "q"}]}]}], "}"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"uo", "[", 
    RowBox[{"q_", ",", "p_", ",", "h_"}], "]"}], ":=", 
   RowBox[{"{", 
    RowBox[{"q", ",", 
     RowBox[{
      RowBox[{
       SuperscriptBox["\[ExponentialE]", 
        RowBox[{
         RowBox[{"-", "\[Gamma]"}], " ", "h"}]], "p"}], "+", 
      RowBox[{
       RowBox[{"Sqrt", "[", 
        RowBox[{"k", " ", "T", 
         RowBox[{"(", 
          RowBox[{"1", "-", 
           SuperscriptBox["\[ExponentialE]", 
            RowBox[{
             RowBox[{"-", "2"}], "\[Gamma]", " ", "h"}]]}], ")"}], "m"}], 
        "]"}], "R"}]}]}], "}"}]}], ";"}]}], "Input",
