     "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{
       RowBox[{
        RowBox[{
        "To", " ", "ensure", " ", "that", " ", "our", " ", "final", " ", 
         "plot", " ", "has", " ", "a", " ", "1"}], ":", 
        RowBox[{"1", " ", "coordinate", " ", "ratio"}]}], ",", " ", 
       RowBox[{
       "we", " ", "find", " ", "the", " ", "smallest", " ", "and", " ", 
        "largest", " ", "x", " ", "and", " ", "y", " ", "values"}], ",", " ", 
       
       RowBox[{"respectively", "."}]}], " ", "*)"}], "\[IndentingNewLine]", 
     RowBox[{"xMin", "=", 
      RowBox[{"init", "[", 
       RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"xMax", "=", 
      RowBox[{"init", "[", 
       RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"yMin", "=", 
      RowBox[{"init", "[", 
       RowBox[{"[", "2", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"yMax", "=", 
      RowBox[{"init", "[", 
       RowBox[{"[", "2", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"direction", "=", 
      RowBox[{"OptionValue", "[", "WalkSpace", "]"}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"While", "[", 
      RowBox[{
       RowBox[{"curStep", "\[LessEqual]", "steps"}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"curSol", "=", 
         RowBox[{"solutions", "[", 
          RowBox[{"[", 
           RowBox[{"curStep", "-", "1"}], "]"}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"dir", "=", 
         RowBox[{"direction", "[", "]"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"newSol", "=", 
         RowBox[{"curSol", "+", 
          RowBox[{"distance", " ", 
           RowBox[{"direction", "[", "]"}]}]}]}], ";", "\[IndentingNewLine]", 
        "\[IndentingNewLine]", 
        RowBox[{"(*", " ", 
         RowBox[{"xMin", ",", " ", "xMax", ",", " ", "yMin", ",", " ", 
          RowBox[{
           RowBox[{"yMax", " ", "are", " ", "re"}], "-", 
           RowBox[{"evaluated", " ", "at", " ", "every", " ", 
            RowBox[{"iteration", "."}]}]}]}], " ", "*)"}], 
        "\[IndentingNewLine]", 
        RowBox[{"xMin", "=", 
         RowBox[{"Min", "[", 
          RowBox[{"xMin", ",", 
