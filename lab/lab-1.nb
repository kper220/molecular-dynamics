            RowBox[{"[", "1", "]"}], "]"}]}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"xMax", "=", 
         RowBox[{"Max", "[", 
          RowBox[{"xMax", ",", 
           RowBox[{"newSol", "[", 
            RowBox[{"[", "1", "]"}], "]"}]}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"yMin", "=", 
