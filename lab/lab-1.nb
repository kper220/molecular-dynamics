     RowBox[{"(*", " ", 
      RowBox[{
      "The", " ", "purpose", " ", "of", " ", "degrees", " ", "is", " ", "to", 
       " ", "select", " ", "a", " ", "range", " ", "of", " ", "points", " ", 
       "with", " ", "respect", " ", "to", " ", "degree", " ", "away", " ", 
       "from", " ", "the", " ", 
       RowBox[{"first", "."}]}], " ", "*)"}], "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"start", ",", "end"}], "}"}], "=", 
