Cell[BoxData[
 RowBox[{
  RowBox[{"IdealSquare", "[", 
   RowBox[{"xMin_", ",", "xMax_", ",", "yMin_", ",", "yMax_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"dx", "=", 
      FractionBox[
       RowBox[{"xMax", "-", "xMin"}], "2"]}], ";", "\[IndentingNewLine]", 
     RowBox[{"cx", "=", 
      RowBox[{"Mean", "[", 
       RowBox[{"{", 
        RowBox[{"xMin", ",", "xMax"}], "}"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"dy", "=", 
      FractionBox[
       RowBox[{"yMax", "-", "yMin"}], "2"]}], ";", "\[IndentingNewLine]", 
     RowBox[{"cy", "=", 
      RowBox[{"Mean", "[", 
       RowBox[{"{", 
        RowBox[{"yMin", ",", "yMax"}], "}"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Which", "[", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"dx", "\[GreaterEqual]", "dy"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"yNewMin", "=", 
         RowBox[{"cy", "-", "dx"}]}], ";", 
        RowBox[{"yNewMax", "=", 
         RowBox[{"cy", "+", "dx"}]}], ";", 
        RowBox[{"{", 
         RowBox[{"xMin", ",", "xMax", ",", "yNewMin", ",", "yNewMax"}], 
         "}"}]}], ",", "\[IndentingNewLine]", 
       RowBox[{"dy", ">", "dx"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"xNewMin", "=", 
         RowBox[{"cx", "-", "dy"}]}], ";", 
        RowBox[{"xNewMax", "=", 
         RowBox[{"cx", "+", "dy"}]}], ";", 
        RowBox[{"{", 
         RowBox[{"xNewMin", ",", "xNewMax", ",", "yMin", ",", "yMax"}], 
         "}"}]}]}], "\[IndentingNewLine]", "]"}]}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",
