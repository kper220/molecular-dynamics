Cell[BoxData[
 RowBox[{
  RowBox[{"IdealSquare", "[", 
   RowBox[{"xMin_", ",", "xMax_", ",", "yMin_", ",", "yMax_"}], "]"}], ":=", 
  RowBox[{"If", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"yMax", "-", "yMin"}], "\[GreaterEqual]", 
     RowBox[{"xMax", "-", "xMin"}]}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"xMean", "=", 
      RowBox[{"Mean", "[", 
       RowBox[{"{", 
        RowBox[{"xMin", ",", "xMax"}], "}"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"dx", "=", 
      FractionBox[
       RowBox[{"yMax", "-", "yMin"}], "2"]}], ";", "\[IndentingNewLine]", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"xMean", "-", "dx"}], ",", 
       RowBox[{"xMean", "+", "dx"}], ",", "yMin", ",", "yMax"}], "}"}]}], ",",
     "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"yMean", "=", 
      RowBox[{"Mean", "[", 
       RowBox[{"{", 
        RowBox[{"yMin", ",", "yMax"}], "}"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"dy", "=", 
      FractionBox[
       RowBox[{"xMax", "-", "xMin"}], "2"]}], ";", "\[IndentingNewLine]", 
     RowBox[{"{", 
      RowBox[{"xMin", ",", "xMax", ",", 
       RowBox[{"yMean", "-", "dy"}], ",", 
       RowBox[{"yMean", "+", "dy"}]}], "}"}]}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",
