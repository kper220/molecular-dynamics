Cell[CellGroupData[{

Cell["Stochastic differential equations", "Subsection",
 CellChangeTimes->{{3.768903069464982*^9, 
  3.768903073861741*^9}},ExpressionUUID->"d2d290ac-4335-493b-a8f7-\
44436901ccb8"],

Cell[TextData[{
 "An ordinary differential equation ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     StyleBox["dx",
      FontSlant->"Italic"], "(", "t", ")"}], "=", 
    RowBox[{
     RowBox[{"f", "(", 
      RowBox[{"t", ",", "x"}], ")"}], 
     StyleBox["dt",
      FontSlant->"Italic"]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "f886e604-5b31-4eab-a29f-4b5b6190bb6b"],
 " with initial conditions ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"x", "(", "0", ")"}], "=", 
    SubscriptBox["x", "0"]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "db5203bf-ae66-4456-990c-745ce6007419"],
 " has integral form ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"x", "(", "t", ")"}], "=", 
    RowBox[{
     SubscriptBox["x", "0"], "+", 
     RowBox[{"\[Integral]", 
      RowBox[{
       RowBox[{"f", "(", 
        RowBox[{"s", ",", 
         RowBox[{"x", "(", "s", ")"}]}], ")"}], 
       StyleBox["ds",
        FontSlant->"Italic"]}]}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "071d5c71-36eb-4576-8d1b-4dec793bbb9a"],
 ", where ",
 Cell[BoxData[
  FormBox[
   RowBox[{"x", "(", 
    RowBox[{"t", ",", 
     SubscriptBox["x", "0"], ",", 
     SubscriptBox["t", "0"]}], ")"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "09f85820-25fd-4a88-8601-6c11a21fb843"],
 " is the solution with initial conditions ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"x", "(", 
     SubscriptBox["t", "0"], ")"}], "=", 
    SubscriptBox["x", "0"]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "5a5d655b-261a-4e47-9db2-b31a0ce0ab41"],
 ". An example ODE is ",
 Cell[BoxData[
  FormBox[
   StyleBox[
    RowBox[{
     RowBox[{"dx", "(", "t", ")"}], "=", 
     RowBox[{
      RowBox[{"a", "(", "t", ")"}], 
      RowBox[{"x", "(", "t", ")"}], "dt"}]}],
    FontSlant->"Italic"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "d348ba04-3472-46ee-967d-9ee71d92c56a"],
 ". This system is separable. A stochastic differential equation (SDE) \
assumes that ",
 Cell[BoxData[
  FormBox[
   RowBox[{"a", "(", "t", ")"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "b83d049d-bbb4-49e2-854d-2dd06dc19347"],
 " is not deterministic but stochastic, so that ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"a", "(", "t", ")"}], "=", 
    RowBox[{
     RowBox[{"f", "(", "t", ")"}], "+", 
     RowBox[{"\[Xi]", "(", "t", ")"}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "62261894-11e5-40f3-8795-0a6fde185adf"],
 ", and ",
 Cell[BoxData[
  FormBox[
   StyleBox[
    RowBox[{
     RowBox[{"dx", "(", "t", ")"}], "=", 
     RowBox[{
      RowBox[{
       RowBox[{"f", "(", "t", ")"}], 
       RowBox[{"x", "(", "t", ")"}], "dt"}], "+", 
      RowBox[{
       RowBox[{"\[Xi]", "(", "t", ")"}], 
       RowBox[{"x", "(", "t", ")"}], "dt"}]}]}],
    FontSlant->"Italic"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "51521dba-40dc-45af-ada3-bd57d9f4ba6a"],
 ", where ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Xi]", "(", "t", ")"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "bc651713-2b9b-4da1-b1dc-cd5cec3fc266"],
 " is a white noise process. Let ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    StyleBox[
     RowBox[{"dW", "(", "t", ")"}],
     FontSlant->"Italic"], 
    StyleBox["=",
     FontSlant->"Italic"], 
    StyleBox[
     RowBox[{
      RowBox[{"\[Xi]", "(", "t", ")"}], "dt"}],
     FontSlant->"Plain"]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "9c96e73e-f608-419a-a341-59b632446848"],
 ", then ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     StyleBox["dx",
      FontSlant->"Italic"], "(", "t", ")"}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"f", "(", "t", ")"}], 
      RowBox[{"x", "(", "t", ")"}], 
      StyleBox["dt",
       FontSlant->"Italic"]}], "+", 
     RowBox[{
      RowBox[{"x", "(", "t", ")"}], 
      RowBox[{
       StyleBox["dW",
        FontSlant->"Italic"], "(", "t", ")"}]}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "6ae8bf41-ffb9-49ac-b48e-288eccd9d553"],
 ". ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    StyleBox["dW",
     FontSlant->"Italic"], "(", "t", ")"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "aea04fda-3d8a-4ddf-aeae-5ad4119df740"],
 " denotes the differential form of the Brownian motion."
}], "Text",
