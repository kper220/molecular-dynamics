     "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{
       RowBox[{
        RowBox[{
        "To", " ", "ensure", " ", "that", " ", "our", " ", "final", " ", 
         "plot", " ", "has", " ", "a", " ", "1"}], ":", 
        RowBox[{"1", " ", "coordinate", " ", "ratio"}]}], ",", " ", 
       RowBox[{
       "we", " ", "find", " ", "the", " ", "smallest", " ", "and", " ", 
        "largest", " ", "x", " ", "and", " ", "y", " ", "values"}], ",", " ", 
       
       RowBox[{"respectively", "."}]}], " ", "*)"}], "\[IndentingNewLine]", 
     RowBox[{"xMin", "=", 
      RowBox[{"init", "[", 
       RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"xMax", "=", 
      RowBox[{"init", "[", 
       RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"yMin", "=", 
      RowBox[{"init", "[", 
       RowBox[{"[", "2", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"yMax", "=", 
      RowBox[{"init", "[", 
       RowBox[{"[", "2", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"While", "[", 
      RowBox[{
       RowBox[{"n", "\[LessEqual]", "steps"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"curSol", "=", 
         RowBox[{"solutions", "[", 
          RowBox[{"[", 
           RowBox[{"n", "-", "1"}], "]"}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"newSol", "=", 
         RowBox[{"curSol", "+", 
          RowBox[{"distance", " ", 
           RowBox[{"direction", "[", "]"}]}]}]}], ";", "\[IndentingNewLine]", 
        "\[IndentingNewLine]", 
        RowBox[{"(*", " ", 
         RowBox[{"xMin", ",", " ", "xMax", ",", " ", "yMin", ",", " ", 
          RowBox[{
           RowBox[{"yMax", " ", "are", " ", "re"}], "-", 
           RowBox[{"evaluated", " ", "at", " ", "every", " ", 
            RowBox[{"iteration", "."}]}]}]}], " ", "*)"}], 
        "\[IndentingNewLine]", 
        RowBox[{"xMin", "=", 
         RowBox[{"Min", "[", 
          RowBox[{"xMin", ",", 
           RowBox[{"newSol", "[", 
            RowBox[{"[", "1", "]"}], "]"}]}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"xMax", "=", 
         RowBox[{"Max", "[", 
          RowBox[{"xMax", ",", 
           RowBox[{"newSol", "[", 
            RowBox[{"[", "1", "]"}], "]"}]}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"yMin", "=", 
         RowBox[{"Min", "[", 
          RowBox[{"yMin", ",", 
           RowBox[{"newSol", "[", 
            RowBox[{"[", "2", "]"}], "]"}]}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"yMax", "=", 
         RowBox[{"Max", "[", 
          RowBox[{"yMax", ",", 
           RowBox[{"newSol", "[", 
            RowBox[{"[", "2", "]"}], "]"}]}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"AppendTo", "[", 
         RowBox[{"solutions", ",", "newSol"}], "]"}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"n", "+=", "1"}]}]}], "]"}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{"Calculate", " ", "ideal", " ", 
       RowBox[{"square", "."}]}], " ", "*)"}], "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"xMin", ",", "xMax", ",", "yMin", ",", "yMax"}], "}"}], "=", 
      RowBox[{"IdealSquare", "[", 
       RowBox[{"xMin", ",", "xMax", ",", "yMin", ",", "yMax"}], "]"}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"solutions", "=", 
      RowBox[{
       RowBox[{
        RowBox[{"Level", "[", 
         RowBox[{
          RowBox[{"solutions", "[", 
           RowBox[{"[", "#", "]"}], "]"}], ",", 
          RowBox[{"{", 
           RowBox[{"-", "1"}], "}"}]}], "]"}], "&"}], "/@", 
       RowBox[{"Range", "[", "steps", "]"}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"OptionValue", "[", "plot", "]"}], ",", "\[IndentingNewLine]", 
       
       RowBox[{"ListPlot", "[", "\[IndentingNewLine]", 
        RowBox[{"solutions", ",", "\[IndentingNewLine]", 
