Cell[BoxData[
 RowBox[{
  RowBox[{"RandomWalk", "[", 
   RowBox[{"init_", ",", "steps_", ",", "distance_", ",", 
    RowBox[{"OptionsPattern", "[", 
     RowBox[{"{", 
      RowBox[{"plot", "\[Rule]", "True"}], "}"}], "]"}]}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"solutions", "=", 
      RowBox[{"{", "init", "}"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"direction", "[", "]"}], ":=", 
      RowBox[{"Module", "[", 
       RowBox[{
        RowBox[{"{", "}"}], ",", 
        RowBox[{
         RowBox[{"t", "=", 
          RowBox[{"RandomReal", "[", 
           RowBox[{"{", 
            RowBox[{"0", ",", 
             RowBox[{"2", "Pi"}]}], "}"}], "]"}]}], ";", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"Cos", "[", "t", "]"}], "}"}], ",", 
           RowBox[{"{", 
            RowBox[{"Sin", "[", "t", "]"}], "}"}]}], "}"}]}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"n", "=", "2"}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{
       RowBox[{
        RowBox[{
        "To", " ", "ensure", " ", "that", " ", "our", " ", "final", " ", 
         "plot", " ", "has", " ", "a", " ", "1"}], ":", 
        RowBox[{"1", " ", "coordinate", " ", "ratio"}]}], ",", " ", 
       RowBox[{
       "we", " ", "find", " ", "the", " ", "smallest", " ", "and", " ", 
        "largest", " ", "x", " ", "and", " ", "y", " ", "values"}], ",", " ", 
       
       RowBox[{"respectively", "."}]}], " ", "*)"}], "\[IndentingNewLine]", 
     RowBox[{"xMin", "=", 
      RowBox[{"init", "[", 
       RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"xMax", "=", 
      RowBox[{"init", "[", 
       RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"yMin", "=", 
      RowBox[{"init", "[", 
       RowBox[{"[", "2", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"yMax", "=", 
      RowBox[{"init", "[", 
       RowBox[{"[", "2", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"While", "[", 
      RowBox[{
       RowBox[{"n", "\[LessEqual]", "steps"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"curSol", "=", 
         RowBox[{"solutions", "[", 
          RowBox[{"[", 
           RowBox[{"n", "-", "1"}], "]"}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"newSol", "=", 
         RowBox[{"curSol", "+", 
          RowBox[{"distance", " ", 
           RowBox[{"direction", "[", "]"}]}]}]}], ";", "\[IndentingNewLine]", 
        "\[IndentingNewLine]", 
        RowBox[{"(*", " ", 
         RowBox[{"xMin", ",", " ", "xMax", ",", " ", "yMin", ",", " ", 
          RowBox[{
           RowBox[{"yMax", " ", "are", " ", "re"}], "-", 
           RowBox[{"evaluated", " ", "at", " ", "every", " ", 
            RowBox[{"iteration", "."}]}]}]}], " ", "*)"}], 
        "\[IndentingNewLine]", 
        RowBox[{"xMin", "=", 
         RowBox[{"Min", "[", 
          RowBox[{"xMin", ",", 
           RowBox[{"newSol", "[", 
            RowBox[{"[", "1", "]"}], "]"}]}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"xMax", "=", 
         RowBox[{"Max", "[", 
          RowBox[{"xMax", ",", 
           RowBox[{"newSol", "[", 
            RowBox[{"[", "1", "]"}], "]"}]}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"yMin", "=", 
         RowBox[{"Min", "[", 
          RowBox[{"yMin", ",", 
           RowBox[{"newSol", "[", 
            RowBox[{"[", "2", "]"}], "]"}]}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"yMax", "=", 
         RowBox[{"Max", "[", 
          RowBox[{"yMax", ",", 
           RowBox[{"newSol", "[", 
            RowBox[{"[", "2", "]"}], "]"}]}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"AppendTo", "[", 
         RowBox[{"solutions", ",", "newSol"}], "]"}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"n", "+=", "1"}]}]}], "]"}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{"Calculate", " ", "ideal", " ", 
       RowBox[{"square", "."}]}], " ", "*)"}], "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"xMin", ",", "xMax", ",", "yMin", ",", "yMax"}], "}"}], "=", 
      RowBox[{"IdealSquare", "[", 
       RowBox[{"xMin", ",", "xMax", ",", "yMin", ",", "yMax"}], "]"}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"solutions", "=", 
      RowBox[{
       RowBox[{
        RowBox[{"Level", "[", 
         RowBox[{
          RowBox[{"solutions", "[", 
           RowBox[{"[", "#", "]"}], "]"}], ",", 
          RowBox[{"{", 
           RowBox[{"-", "1"}], "}"}]}], "]"}], "&"}], "/@", 
       RowBox[{"Range", "[", "steps", "]"}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"OptionValue", "[", "plot", "]"}], ",", "\[IndentingNewLine]", 
       
       RowBox[{"ListPlot", "[", "\[IndentingNewLine]", 
        RowBox[{"solutions", ",", "\[IndentingNewLine]", 
         RowBox[{"AspectRatio", "\[Rule]", "1"}], ",", "\[IndentingNewLine]", 
         
         RowBox[{"Axes", "\[Rule]", "False"}], ",", "\[IndentingNewLine]", 
         RowBox[{"ImageSize", "\[Rule]", "1024"}], ",", "\[IndentingNewLine]", 
         RowBox[{"Joined", "\[Rule]", "True"}], ",", "\[IndentingNewLine]", 
         RowBox[{"PlotRange", "\[Rule]", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"{", 
             RowBox[{"xMin", ",", "xMax"}], "}"}], ",", 
            RowBox[{"{", 
             RowBox[{"yMin", ",", "yMax"}], "}"}]}], "}"}]}], ",", 
         "\[IndentingNewLine]", 
         RowBox[{"PlotStyle", "\[Rule]", "Gray"}]}], "\[IndentingNewLine]", 
        "]"}], ",", "\[IndentingNewLine]", 
       RowBox[{"{", 
        RowBox[{
        "solutions", ",", "xMin", ",", "xMax", ",", "yMin", ",", "yMax"}], 
        "}"}]}], "]"}]}]}], "\[IndentingNewLine]", "]"}]}]], "Input",
 CellChangeTimes->{{3.7688585939571667`*^9, 3.768858844648964*^9}, {
